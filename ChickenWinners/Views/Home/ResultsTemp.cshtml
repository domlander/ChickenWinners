@model IEnumerable<ChickenScratch.Models.Team>

@{
    ViewBag.Title = "Results";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Results</h2>

<div class="row col-md-8">
    <table class="table">
        @for (int i = 0; i < 5; i++)
        {
            <tr>
                <td>Team@(i * 2)</td>
                <td>vs</td>
                <td>Team@(i * 2 + 1)</td>
            </tr>
        }
    </table>
</div>